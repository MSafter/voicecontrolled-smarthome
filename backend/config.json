{
  "name": "Mert's smart home",
  "icon": "home",
  "commands": [
    {
      "keyphrase": "hello",
      "icon": "people",
      "commands": [
        {
          "phrase": "native world",
          "icon": "people",
          "command": {
            "type": "native",
            "exec": "echo hello world",
            "runAsync": true
          }
        },
        {
          "phrase": "node world",
          "icon": "people",
          "command": {
            "type": "node",
            "modulename": "example",
            "functionname": "sayHello",
            "parameters": [
              {
                "type": "plain",
                "value": "world"
              }
            ]
          }
        }
      ]
    },
    {
      "keyphrase": "wake",
      "icon": "devices",
      "commands": [
        {
          "phrase": "ps4",
          "icon": "games",
          "command": {
            "type": "node",
            "modulename": "smarthome",
            "functionname": "psWake"
          }
        },
        {
          "phrase": "pc",
          "icon": "computer",
          "command": {
            "type": "node",
            "modulename": "smarthome",
            "functionname": "wol",
            "parameters": [
              {
                "type": "url",
                "value": "http://localhost:3000/api/device/info"
              }
            ]
          }
        }
      ]
    },
    {
      "keyphrase": "TV",
      "icon": "tv",
      "commands": [
        {
          "phrase": "Change input",
          "icon": "input",
          "command": {
            "node": "smarthome",
            "functionname": "tvChangeInput",
            "parameters": [
              {
                "type": "plain",
                "value": "a"
              },
              {
                "type": "plain",
                "value": "b"
              }
            ]
          }
        }
      ]
    }
  ]
}